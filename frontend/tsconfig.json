{
	"extends": "@vue/tsconfig/tsconfig.json",
	"include": [
		"auto-imports.d.ts",
		"components.d.ts",
		"src/**/*.ts",
		"src/**/*.d.ts",
		"src/**/*.tsx",
		"src/**/*.vue"
	],
	"compilerOptions": {
		"types": ["vite/client"],
		"strict": false,
		"allowJs": true,
		"preserveValueImports": false,
		"importsNotUsedAsValues": "remove"
	}
}
